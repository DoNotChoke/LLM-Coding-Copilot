{
  "name": "llm-coding-agent",
  "displayName": "llm-coding-agent",
  "description": "VSCode extension for code inline suggestions.",
  "version": "0.0.1",
  "engines": { "vscode": "^1.109.0" },
  "categories": ["Other"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      { "command": "llm-coding-agent.helloWorld", "title": "Hello World" },
      { "command": "llm-coding-agent.inlineSuggest", "title": "LLM: Inline Suggest" }
    ],
    "keybindings": [
      {
        "key": "alt+s",
        "command": "llm-coding-agent.inlineSuggest",
        "when": "editorTextFocus",
        "mac": "alt+s"
      }
    ]
  }
}
